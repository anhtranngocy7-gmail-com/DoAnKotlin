    <style name="Theme.Quanlichitieu.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Quanlichitieu.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>